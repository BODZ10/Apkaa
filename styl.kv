<MainWindow>:
    name: 'main'
    
    BoxLayout:
        orientation:'vertical'
        size: root.width, root.height
        padding: ('10dp', '10dp', '10dp', '10dp')
        spacing: '10dp'        

        Button:
            text:'MEMORIES'
            size_hint: 1,.2
            background_color: 1 , 0 , 1 , 1
            on_release: 
                root.manager.current = choice(root.manager.screen_names[1:35])
        Button:
            text:'TÄ˜SKNOTA'
            size_hint: 1,.2
            background_color: 1 , 0 , 1 , 1
            on_release: 
                root.manager.current = choice(root.manager.screen_names[35:45]) 
        Button:
            text:'EXTRAS'
            size_hint: 1,.2
            background_color: 1 , 0 , 1 , 1
            on_release: 
                root.manager.current = choice(root.manager.screen_names[45:])  

